<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hh.his.graduationproject.dao.PaymentTypeMapper">
    <resultMap id="PaymentTypeResultMap" type="PaymentType">
        <result property="paymentNo" column="payment_no"/>
        <result property="paymentType" column="payment_type"/>
    </resultMap>
    <sql id="PaymentTypeColumns">
        payment_no,payment_type
    </sql>

    <select id="findAllPaymentType" resultMap="PaymentTypeResultMap">
        SELECT <include refid="PaymentTypeColumns"/>
        FROM payment_type
    </select>
</mapper>